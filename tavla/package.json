{
    "name": "tavla",
    "version": "0.1.0",
    "private": true,
    "browserslist": [
        "chrome > 47"
    ],
    "scripts": {
        "dev": "next dev --turbopack & firebase emulators:start --project=ent-tavla-dev",
        "dev:persist": "next dev & yarn emulators",
        "emulators": "firebase emulators:start --project=ent-tavla-dev --import=./.db --export-on-exit",
        "build": "next build",
        "build:prod": "NEXT_PUBLIC_ENV=prod next build",
        "typecheck": "tsc --noEmit --incremental false",
        "depcheck": "depcruise src",
        "start": "next start",
        "generate": "graphql-codegen",
        "lint": "next lint --max-warnings 0",
        "prettier": "prettier . --check",
        "fix": "next lint --fix && prettier . --write",
        "build-start": "next build && next start",
        "postinstall": "cd .. && husky install ./.husky"
    },
    "dependencies": {
        "@entur/alert": "0.17.12",
        "@entur/button": "3.3.12",
        "@entur/chip": "0.9.0",
        "@entur/dropdown": "7.3.1",
        "@entur/expand": "^3.6.12",
        "@entur/form": "8.3.1",
        "@entur/icons": "8.0.0",
        "@entur/layout": "3.1.8",
        "@entur/loader": "0.5.29",
        "@entur/menu": "5.2.0",
        "@entur/modal": "1.7.72",
        "@entur/table": "4.9.13",
        "@entur/tokens": "3.19.1",
        "@entur/tooltip": "5.2.12",
        "@entur/typography": "1.9.12",
        "@firebase/database-types": "1.0.16",
        "@floating-ui/react": "0.27.15",
        "@sentry/nextjs": "^8",
        "@types/formidable": "3.4.5",
        "@types/jsdom": "21.1.7",
        "abortcontroller-polyfill": "1.7.8",
        "classnames": "2.5.1",
        "dompurify": "3.2.6",
        "firebase": "11.0.1",
        "firebase-admin": "13.4.0",
        "formidable": "3.5.4",
        "graphql": "16.11.0",
        "jsdom": "26.1.0",
        "lodash": "4.17.21",
        "nanoid": "5.1.5",
        "next": "15.5.0",
        "posthog-js": "1.260.1",
        "prettier-plugin-organize-imports": "4.2.0",
        "prettier-plugin-tailwindcss": "0.6.14",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-google-button": "0.8.0",
        "sharp": "0.34.3",
        "swr": "2.3.6"
    },
    "devDependencies": {
        "@eslint/eslintrc": "3.3.1",
        "@eslint/js": "9.33.0",
        "@graphql-codegen/cli": "5.0.7",
        "@graphql-codegen/import-types-preset": "3.0.1",
        "@graphql-codegen/typed-document-node": "5.1.2",
        "@graphql-codegen/typescript": "4.1.6",
        "@graphql-codegen/typescript-operations": "4.6.1",
        "@types/lodash": "4.17.20",
        "@types/node": "22.8.4",
        "@types/react": "19.0.12",
        "@types/react-dom": "19.0.4",
        "@types/semver": "7.7.0",
        "@typescript-eslint/eslint-plugin": "8.40.0",
        "@typescript-eslint/parser": "8.40.0",
        "autoprefixer": "10.4.21",
        "dependency-cruiser": "16.5.0",
        "eslint": "9.33.0",
        "eslint-config-next": "15.5.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-react": "7.37.5",
        "firebase-tools": "13.27.0",
        "husky": "9.1.7",
        "postcss": "8.5.6",
        "prettier": "3.6.2",
        "tailwindcss": "3.4.14",
        "typescript": "5.9.2"
    },
    "packageManager": "yarn@3.5.0",
    "engines": {
        "node": "18"
    },
    "resolutions": {
        "ip": ">=2.0.1",
        "@types/react": "19.0.12",
        "@types/react-dom": "19.0.4"
    }
}
