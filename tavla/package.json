{
    "name": "tavla",
    "version": "0.1.0",
    "private": true,
    "browserslist": [
        "chrome > 47"
    ],
    "scripts": {
        "dev": "next dev & firebase emulators:start --project=ent-tavla-dev",
        "dev:persist": "next dev & yarn emulators",
        "emulators": "firebase emulators:start --project=ent-tavla-dev --import=./.db --export-on-exit",
        "build": "next build",
        "build:prod": "NEXT_PUBLIC_ENV=prod next build",
        "typecheck": "tsc --noEmit --incremental false",
        "depcheck": "depcruise src",
        "start": "next start",
        "generate": "graphql-codegen",
        "lint": "next lint --max-warnings 0",
        "prettier": "prettier . --check",
        "fix": "next lint --fix && prettier .",
        "build-start": "next build && next start",
        "postinstall": "cd .. && husky install ./.husky"
    },
    "dependencies": {
        "@dnd-kit/core": "6.0.8",
        "@dnd-kit/modifiers": "7.0.0",
        "@dnd-kit/sortable": "8.0.0",
        "@dnd-kit/utilities": "3.2.1",
        "@entur/alert": "0.14.9",
        "@entur/button": "3.2.28",
        "@entur/chip": "0.7.15",
        "@entur/dropdown": "6.0.8",
        "@entur/expand": "3.4.0",
        "@entur/form": "8.1.5",
        "@entur/icons": "7.4.2",
        "@entur/layout": "2.1.33",
        "@entur/loader": "0.4.31",
        "@entur/menu": "5.0.3",
        "@entur/modal": "1.7.3",
        "@entur/tab": "0.4.51",
        "@entur/tokens": "3.15.1",
        "@entur/tooltip": "5.1.1",
        "@entur/travel": "6.0.18",
        "@entur/typography": "1.8.20",
        "@firebase/database-types": "1.0.6",
        "@floating-ui/react": "0.26.12",
        "classnames": "2.3.2",
        "firebase": "11.0.1",
        "firebase-admin": "12.6.0",
        "graphql": "16.6.0",
        "lodash": "4.17.21",
        "nanoid": "5.0.7",
        "next": "15.0.1",
        "pino": "9.4.0",
        "pino-pretty": "11.2.2",
        "posthog-js": "1.155.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "sharp": "0.32.6",
        "swr": "2.1.5"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "5.0.3",
        "@graphql-codegen/import-types-preset": "3.0.0",
        "@graphql-codegen/typed-document-node": "5.0.0",
        "@graphql-codegen/typescript": "4.1.0",
        "@graphql-codegen/typescript-operations": "4.3.0",
        "@types/lodash": "4.14.191",
        "@types/node": "22.7.9",
        "@types/react": "18.2.38",
        "@types/react-dom": "18.2.17",
        "@typescript-eslint/eslint-plugin": "8.11.0",
        "@typescript-eslint/parser": "8.11.0",
        "autoprefixer": "10.4.19",
        "dependency-cruiser": "16.5.0",
        "eslint": "9.13.0",
        "eslint-config-next": "15.0.1",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.2.1",
        "eslint-plugin-react": "7.33.2",
        "firebase-tools": "13.23.0",
        "husky": "9.1.6",
        "postcss": "8.4.38",
        "prettier": "3.3.3",
        "tailwindcss": "3.4.3",
        "typescript": "5.0.2"
    },
    "packageManager": "yarn@3.5.0",
    "engines": {
        "node": "16"
    }
}
